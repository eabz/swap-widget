{
    "name": "swap-widget",
    "version": "0.1.0",
    "description": "A lightweight, flexible React swap widget",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "format": "biome format --write .",
        "lint": "biome lint --write --unsafe ."
    },
    "peerDependencies": {
        "react": ">=16",
        "react-dom": ">=16"
    },
    "devDependencies": {
        "@biomejs/biome": "2.3.8",
        "react": "19.2.0",
        "react-dom": "19.2.0",
        "typescript": "5.9.3",
        "tsup": "8.5.1",
        "@types/react": "19.2.7",
        "@types/react-dom": "19.2.3"
    }
}