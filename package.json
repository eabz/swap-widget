{
  "name": "swap-widget",
  "version": "1.0.0 ",
  "description": "A lightweight, chain-agnostic swap widget with built-in adapters for Uniswap ",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "biome format --write .",
    "lint": "biome lint --write --unsafe ."
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8"
  }
}
